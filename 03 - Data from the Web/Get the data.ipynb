    "semesters_df[\"semester_total\"] = difference_df[\"semester_total\"]\n",
    "semesters_df = semesters_df[pd.notnull(semesters_df['semester_total'])]\n",
    "semesters_df = semesters_df.drop_duplicates(subset=['nom_prénom'])\n",
    "semesters_df"
